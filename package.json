{
	"name": "thaw-angular-component-library",
	"description": "Components for Angular applications",
	"private": false,
	"version": "0.0.1",
	"repository": {
		"type": "git",
		"url": "https://github.com/tom-weatherhead/thaw-angular-component-library.git"
	},
	"homepage": "https://github.com/tom-weatherhead/thaw-angular-component-library#readme",
	"bugs": {
		"url": "https://github.com/tom-weatherhead/thaw-angular-component-library/issues"
	},
	"scripts": {
		"all": "npm run clean && npm i && npm run test-all && npm run pack-and-deploy",
		"all-notests": "npm run clean && npm i && npm run pack-and-deploy",
		"all-for-travis": "npm run clean && npm i && npm run test-all",
		"clean": "rm -rf dist && rm -rf node_modules && rm -f package-lock.json",
		"pack": "npm run packagr && cd dist && npm pack && cd ..",
		"pack-and-deploy": "npm run packagr && cd dist && npm pack && mv *.tgz ../../othello-angular-electron/ && cd ..",
		"ng": "ng",
		"start": "ng serve",
		"build": "ng build",
		"lint": "ng lint",
		"test": "ng test",
		"unittests": "ng test --code-coverage",
		"unittests-headless": "ng test --watch=false --browsers=ChromeHeadless --code-coverage",
		"e2e": "ng e2e",
		"systemtests": "npm run e2e",
		"test-all": "npm run lint && npm run unittests-headless && npm run systemtests",
		"packagr": "ng-packagr -p ng-package.json"
	},
	"dependencies": {
		"@angular/common": "^8.2.14",
		"@angular/compiler": "^8.2.14",
		"@angular/core": "^8.2.14",
		"@angular/forms": "^8.2.14",
		"@angular/platform-browser": "^8.2.14",
		"@angular/platform-browser-dynamic": "^8.2.14",
		"@angular/router": "^8.2.14",
		"@types/chart.js": "^2.9.0",
		"chart.js": "^2.9.3",
		"chromedriver": "^73.0.0",
		"rxjs": "^6.5.3",
		"tslib": "^1.10.0",
		"zone.js": "0.9.1"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "^0.803.19",
		"@angular/cli": "^8.3.19",
		"@angular/compiler-cli": "^8.2.14",
		"@types/jasmine": "^3.4.6",
		"@types/jasminewd2": "^2.0.8",
		"@types/jquery": "^3.3.31",
		"@types/node": "^12.12.6",
		"codelyzer": "^5.2.0",
		"jasmine-core": "^3.5.0",
		"jasmine-spec-reporter": "^4.2.1",
		"karma": "^4.4.1",
		"karma-chrome-launcher": "^3.1.0",
		"karma-coverage-istanbul-reporter": "^2.1.0",
		"karma-jasmine": "^2.0.1",
		"karma-jasmine-html-reporter": "^1.4.2",
		"ng-packagr": "^5.7.1",
		"protractor": "^5.4.2",
		"selenium-webdriver": "^4.0.0-alpha.5",
		"ts-node": "^8.4.1",
		"tslint": "^5.20.1",
		"typescript": "<3.6"
	},
	"engineStrict": true,
	"engines": {
		"node": ">= 10.9"
	},
	"keywords": [
		"Angular",
		"TypeScript"
	],
	"author": "Tom Weatherhead <null@2hrd4u.org> (https://httpbin.org/status/418)",
	"license": "MIT"
}
